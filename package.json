  "dependencies": {
    "axios": "0.21.1",
    "cheerio": "1.0.0-rc.3",
    "dotenv": "8.1.0",
    "node-cron": "2.0.3",
    "node-localstorage": "1.3.1",
    "onesignal-node": "^2.1.1",
    "puppeteer": "1.19.0",
    "pushover-notifications": "1.2.0"
  },
  "devDependencies": {
    "pm2": "3.5.1"
  }
}
