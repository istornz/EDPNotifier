  "scripts": {
    "fetch": "node main.js",
    "pm2:start:fetch": "pm2 start ./main.js -i 1 --name \"Cron-Fetcher\"",
    "pm2:stop:fetch": "pm2 stop Cron-Fetcher && pm2 delete Cron-Fetcher"
